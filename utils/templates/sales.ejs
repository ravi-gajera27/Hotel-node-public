  <body>
    <div id="container">
      <div class="right-eod">
        <section id="memo">
          <div class="company-info">
            <div><%= rest.rest_name %></div>
            <br />
            <span><%= rest.add_1 %></span>
            <br />
            <span><%= rest.add_2 %></span>
            <br />
            <span><%= rest.city %>, <%= rest.state %></span>
            <br />
            <span><%= rest.mobile_no %></span>
            <br />
            <span><%= rest.email %></span>
          </div>

          <div class="logo">
            <!-- <img src="https://drive.google.com/thumbnail?id=1ioUBhy_R6D3rrRjplj2OXIJKY9cfqGOP" /> -->
            <img
              src="https://drive.google.com/thumbnail?id=<%= rest.logo %>"
            />
          </div>
        </section>

        <section id="invoice-title-number">
          <div class="title-top">
            <!-- <span>22/10/2028</span>  -->
            <span><%= start_date %> - <%= end_date %></span>
          </div>

          <div id="title">SALES REPORT</div>
        </section>

        <div class="clearfix"></div>

        <div class="clearfix"></div>

        <div class="currency">
          <span>* All prices are in</span> <span>INR</span>
        </div>

        <section id="items">
          <table cellpadding="0" cellspacing="0">
            <tr>
              <th>INVOICE ID</th>
              <th>CUSTOMER</th>
              <th>TYPE</th>
              <th>GROSS</th>
              <th>DISCOUNT</th>
              <Th>Tax</Th>
              <th>NET</th>
              <th>CASH</th>
              <th>CARD</th>
              <th>ONLINE</th>
              <th>CREDIT</th>
            </tr>

               <% for(let item of invoice_array) { %>
            </tr>

            <tr>
              <td><%= item.inv_no %></td>
              <td><%= item.cname %></td>
              <td><%= item.type %></td>
              <td><%= item.gross.toFixed(2) %></td>
              <td><%= item.dis ? item.dis.toFixed(2) : '-' %></td>
              <td><%= item.tax.toFixed(2) %></td>
              <td><%= item.total_amt.toFixed(2) %></td>
              <td><%= item.cash ? item.cash.toFixed(2) : '-' %></td>
              <td><%= item.card ? item.card.toFixed(2) : '-' %></td>
              <td><%= item.online ? item.online.toFixed(2) : '-' %></td>
              <td>
                <%= item.settle.credit ? ( item.settle.credit).toFixed(2)  : '-' %>
              </td>
            </tr>
            <% } %>

            <th>TOTAL</th>
            <th><%= total.total_cust %></th>
            <th>-</th>
            <th><%=total.total_gross.toFixed(2) %></th>
            <th><%=total.total_discount.toFixed(2)  %></th>
            <th><%=total.total_tax.toFixed(2)  %></th>
            <th><%=total.total_net.toFixed(2)  %></th>
            <th><%=total.total_cash.toFixed(2)  %></th>
            <th><%=total.total_card.toFixed(2)  %></th>
            <th><%=total.total_online.toFixed(2)  %></th>
            <th><%=total.total_credit.toFixed(2)  %></th>
          </table>
        </section>
        <div class="currency">
          <span></span> <span></span>
        </div>
        <section id="invoice-title-number">
              
            <div id="title">TOTAL SALES REPORT</div>
          </section>
          
          <div class="clearfix"></div>
  
          <div class="clearfix"></div>
  
          <div class="currency">
            <span></span> <span></span>
          </div>
          <section id="items">
            <table cellpadding="0" cellspacing="0">
  
                <tr>
                    <th>TYPE</th>
                    <th>INVOICE</th>
                    <th>GROSS</th>
                    <th>DISCOUNT</th>
                    <Th>Tax</Th>
                    <th>NET</th>
                    <th>CASH</th>
                    <th>CARD</th>
                    <th>ONLINE</th>
                    <th>CREDIT</th>
                  </tr>
                     <% for(let total of totalType) { %>
                  </tr>
      
                  <tr>
                    <td><%= total.type %></td>
                    <td><%= total.total_cust %></td>
                    <td><%=total.total_gross.toFixed(2) %></td>
                    <td><%=total.total_discount.toFixed(2)  %></td>
                    <td><%=total.total_tax.toFixed(2)  %></td>
                    <td><%=total.total_net.toFixed(2)  %></td>
                    <td><%=total.total_cash.toFixed(2)  %></td>
                    <td><%=total.total_card.toFixed(2)  %></td>
                    <td><%=total.total_online.toFixed(2)  %></td>
                    <td><%=total.total_credit.toFixed(2)  %></td>
                  </tr>
                  <% } %>
             
            </table>
          </section>
        <section id="terms">
          <div style="font-weight: bold; font-size: medium">
            Thank You for choosing us.
          </div>
        </section>
      </div>
