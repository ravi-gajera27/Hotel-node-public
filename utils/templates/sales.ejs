        margin-top: 10px;
      }
      #invoice-info div {
        margin-bottom: 3px;
      }
      #invoice-info div span {
        display: inline-block;
        min-width: 20px;
        min-height: 18px;
      }
      #invoice-info div span:first-child {
        font-weight: bold;
        text-transform: uppercase;
        margin-right: 10px;
      }
      #invoice-info:after {
        content: "";
        display: block;
        clear: both;
      }

      .currency {
        margin-top: 20px;
        text-align: right;
        color: #858585;
        font-style: italic;
        font-size: 12px;
      }
      .currency span {
        display: inline-block;
        min-width: 20px;
      }

      #items {
        margin-top: 10px;
      }
      #items .first-cell,
      #items table th:first-child,
      #items table td:first-child {
        width: 10%;
        text-align: left;
      }

      #items table {
        border-collapse: separate;
        width: 100%;
      }
      #items table th {
        font-family: Sanchez, Serif;
        font-size: 10px;
        text-transform: uppercase;
        padding: 5px 3px;
        text-align: right;
        background: #b0b4b3;
        color: black;
        text-align: left;
        text-overflow: ellipsis;
      }
      #items table th:nth-child(2) {
        width: 10%;
        text-align: left;
      }
      #items table td:nth-child(2) {
        text-align: left;
        width: 10%;
      }
      #items table th:last-child {
        text-align: left;
      }
      #items table td {
        padding: 10px 3px;
        text-align: left;
        border-bottom: 1px solid #ddd;
        text-overflow: ellipsis;
    
      }
      #items table td:first-child {
        text-align: left;
      }

      #terms {
        margin-top: 60px;
      }
      #terms > span {
        font-weight: bold;
        display: inline-block;
        width: auto;
        text-transform: uppercase;
      }
      #terms > div {
        height: auto;
        width: auto;
      }

